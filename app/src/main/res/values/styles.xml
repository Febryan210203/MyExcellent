<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.SplashScreen" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/colorWhite</item>
        <item name="colorPrimaryVariant">@color/colorWhite</item>
        <item name="colorOnPrimary">@color/colorWhite</item>
        <item name="android:statusBarColor">@color/light_blue</item>
    </style>
    <style name="SpinnerPopupTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textColor">#000000</item>
    </style>
    <style name="TextLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:alpha">0.7</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <string-array name="jenis_kelamin_array">
        <item>Laki-laki</item>
        <item>Perempuan</item>
    </string-array>

</resources>